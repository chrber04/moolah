{
  "name": "@moolah/domain",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./app": {
      "types": "./dist/app/index.d.ts",
      "import": "./dist/app/index.js",
      "default": "./dist/app/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "import": "./dist/auth/index.js",
      "default": "./dist/auth/index.js"
    },
    "./blog": {
      "types": "./dist/blog/index.d.ts",
      "import": "./dist/blog/index.js",
      "default": "./dist/blog/index.js"
    },
    "./bot": {
      "types": "./dist/bot/index.d.ts",
      "import": "./dist/bot/index.js",
      "default": "./dist/bot/index.js"
    },
    "./emoji": {
      "types": "./dist/emoji/index.d.ts",
      "import": "./dist/emoji/index.js",
      "default": "./dist/emoji/index.js"
    },
    "./report": {
      "types": "./dist/report/index.d.ts",
      "import": "./dist/report/index.js",
      "default": "./dist/report/index.js"
    },
    "./review": {
      "types": "./dist/review/index.d.ts",
      "import": "./dist/review/index.js",
      "default": "./dist/review/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./user": {
      "types": "./dist/user/index.d.ts",
      "import": "./dist/user/index.js",
      "default": "./dist/user/index.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "clean": "rimraf .turbo dist node_modules",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@moolah/config-eslint": "workspace:^",
    "@moolah/config-prettier": "workspace:^",
    "@moolah/config-typescript": "workspace:^",
    "typescript": "*"
  }
}
